@model List<Services>
<a class="btn btn-success" href="/dashboard/Service/Create">Create</a>

<table class="table table-hover table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Icon</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Title</td>
                <td>
                    <i class="@item.Icon"></i>
                </td>
                <td>
                    <a class="btn btn-dark btn-outline-primary" href="/Dashboard/Service/Edit/@item.Id">Edit</a>
                    <a class="btn btn-danger btn-outline-success" href="/Dashboard/Service/Delete/@item.Id">Delete</a>
                    <a class="btn btn-warning btn-outline-danger">Detail</a>
                </td>
            </tr>
        }


    </tbody>
</table>